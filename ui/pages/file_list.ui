      <string notr="true">QTreeWidget {
    spacing: 10px; /* Cách nhau 5px giữa các item trong 1 hàng */
}

#files{
	background-color: #3E7CB1;
	border:1px solid #A0B9D0;
	border-radius: 25px solid #DBE4EE;
	padding:25px;
	color: #DBE4EE;
}

QHeaderView::section
{	color: #DBE4EE;
	background-color: #3E7CB1;
	border:1px;
	border-top-right:5px;
	padding-left:10px;
	padding-bottom:20px		
}

QTreeWidget::item{
	padding-bottom:5px;		
	spacing: 10px;
}

QTreeWidget::item::hover{
	background-color: #81A4CD;
	border:1px solid #81A4CD ;
	border-radius: 5px
}

QTreeWidget::item::pressed{
	font-weight: bold;
	padding-left:5px;
	background-color: #81A4CD;
	border:1px solid #81A4CD ;
	border-radius: 5px
}

QTreeWidget::item::selected{
	font-weight: bold;
	background-color: #81A4CD;
	border:1px solid #81A4CD ;
	border-radius: 5px
}

QTreeWidget::item::selected::active{
	background-color: #054A91;
	border:1px solid #81A4CD ;
	border-radius: 5px;
	font-weight:bold;
}

QScrollArea{
	border:none;
	padding-right:15px;
}

QScrollBar:vertical {
    border:none;
    background: #3E7CB1;
    width: 8px;
	border-radius:5px;
	padding-top:5px;
	padding-left:2px;
}

QScrollBar::handle:vertical {
    background: #A0B9D0;
    min-height: 20px;
	border-radius:3px;
	padding-left: 15px
}

QScrollBar::add-line:vertical {
    border:none;
    background: #A0B9D0;
    height: 0px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    border:none;
    background: #A0B9D0;
    height: 0px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::add-page:vertical {
	background: #3E7CB1;
}

QScrollBar::sub-page:vertical {
	background: #3E7CB1;
}

/*Horizontal scrollbar*/
QScrollBar:horizontal {
    border: none;
    background: #3E7CB1;
    height: 8px;
    border-radius: 5px;
    padding-top: 2px;
    padding-left: 5px;
}

QScrollBar::handle:horizontal {
    background: #A0B9D0;
    min-width: 20px;
    border-radius: 3px;
    padding-left: 5px;
}

QScrollBar::add-line:horizontal {
    border: none;
    background: #A0B9D0;
    width: 0px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    border: none;
    background: #A0B9D0;
    width: 0px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::add-page:horizontal {
    background: #3E7CB1;
}

QScrollBar::sub-page:horizontal {
    background: #3E7CB1;
}</string>
